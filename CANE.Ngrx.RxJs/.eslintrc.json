{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
          "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "semi": ["error", "always"],
        // "quotes": ["error", "double"],
        "quotes": 0,
        "linebreak-style": 0,
        "indent": 0,
        "eol-last": 0        
    }
}

